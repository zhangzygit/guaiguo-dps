<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >
<classPathEntry location="E:\repo\mysql\mysql-connector-java\5.1.45\mysql-connector-java-5.1.45.jar" />
  <context id="default" targetRuntime="MyBatis3">

   <!-- 指定生成的java文件的编码,没有直接生成到项目时中文可能会乱码 -->
    <property name="javaFileEncoding" value="UTF-8"/>
      <!-- 这里的type里写的是你的实现类的类全路径 -->
    <commentGenerator>
       <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
        <property name="suppressAllComments" value="true" />
    </commentGenerator>
  
    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql://127.0.0.1:3306/guaiguodpsdb?useSSL=false"
        userId="root"
        password="root" >
        <property name="useInformationSchema" value="true"></property>
    </jdbcConnection>
            
<!-- 类型转换 -->  
    <javaTypeResolver>  
        <!-- 是否使用bigDecimal， false可自动转化以下类型（Long, Integer, Short, etc.） -->  
        <property name="forceBigDecimals" value="false"/>  
    </javaTypeResolver> 
         
<!-- generate Model -->
    <javaModelGenerator targetPackage="top.guaiguo.model"
                        targetProject="src/main/java">
        <property name="enableSubPackages" value="true" />
        <property name="trimStrings" value="true" />
    </javaModelGenerator>
         
<!-- generate xml -->
    <sqlMapGenerator targetPackage="mappers"
                    targetProject="src/main/resources">
        <property name="enableSubPackages" value="false" />
    </sqlMapGenerator>
         
<!-- generate Mapper  -->
    <javaClientGenerator type="XMLMAPPER"
                  targetPackage="top.guaiguo.dao"
                  targetProject="src/main/java">
        <property name="enableSubPackages" value="true" />
    </javaClientGenerator>
	
    <!--<table schema="wmsinvdb" tableName="purchase_recv_detail" mapperName=""
        enableCountByExample="false" enableUpdateByExample="false"
        enableDeleteByExample="false" enableSelectByExample="false"
        selectByExampleQueryId="false">
      &lt;!&ndash;<columnOverride column="DOUBLE" jdbcType="DECIMAL" />  &ndash;&gt;
    </table>-->

     <!--指定数据库所有表-->
		<table schema="" tableName="%"
        enableCountByExample="false" enableUpdateByExample="false"
        enableDeleteByExample="false" enableSelectByExample="false"
        selectByExampleQueryId="false">
    </table>
  </context>
</generatorConfiguration>
